{
    "name": "circular-bot-scraper",
    "version": "1.0.0",
    "description": "Web scraper for circular.bot",
    "main": "dist/index.js",
    "scripts": {
        "start": "ts-node src/index.ts",
        "build": "tsc",
        "dev": "nodemon --legacy-watch --watch src/**/*.ts --exec ts-node src/index.ts",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest"
    },
    "dependencies": {
        "@solana/web3.js": "^1.87.6",
        "express": "^4.18.2",
        "puppeteer-core": "^21.0.0",
        "@sparticuz/chromium": "^109.0.5",
        "node-cron": "^3.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^18.0.0",
        "@types/node-cron": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.0",
        "prettier": "^2.0.0",
        "typescript": "^4.0.0",
        "ts-node": "^10.9.2",
        "jest": "^27.0.0",
        "@types/jest": "^27.0.0",
        "ts-jest": "^27.0.0",
        "nodemon": "^3.0.3"
    }
}